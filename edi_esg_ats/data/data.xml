<odoo>
	<record model="ir.cron" id="ats_cron">
        <field name="name">EDI : ESG ATS</field>
        <field name="model_id" ref="model_edi_esg_ats"/>
        <field name="state">code</field>
        <field name="code">model.import_ats()</field>
        <field name="interval_number">1</field>
